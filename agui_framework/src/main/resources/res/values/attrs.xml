<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme">
		<!-- Drawable used as a background for activated items. -->
        <attr name="activatedBackgroundIndicator" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="Window">
    	<attr name="windowBackground" format="string" />
     	<attr name="windowIsFloating" format="boolean" />
     	<!-- not use -->
     	<attr name="windowNoTitle" format="boolean" />
     	<attr name="windowFrame" format="reference" />
     	<attr name="windowFullscreen" format="boolean" />
     	<attr name="windowOverscan" format="boolean" />
     	<attr name="windowContentOverlay" format="reference" />
     	<attr name="windowShowWallpaper" format="boolean" />
     	<attr name="windowTitleStyle" format="reference" />
     	<attr name="windowTitleSize" format="dimension" />
     	<attr name="windowTitleBackgroundStyle" format="reference" />
     	<attr name="windowAnimationStyle" format="reference" />
     	<attr name="windowSoftInputMode" format="integer" />
     	<attr name="windowActionBar" format="boolean" />
     	<attr name="windowActionModeOverlay" format="boolean" />
     	<attr name="windowCloseOnTouchOutside" format="boolean" />
     	<attr name="windowTranslucentStatus" format="boolean" />
     	<attr name="windowTranslucentNavigation" format="boolean" />
    </declare-styleable> 
<!--  
	Widget
-->
	<declare-styleable name="fragment">
        <attr name="class" format="string" />
        <attr name="label" format="string" />
    </declare-styleable>

	<declare-styleable name="include">
        <attr name="layout" format="reference" />
    </declare-styleable>
    
	<declare-styleable name="View">
        <attr name="id" format="string" />
        <attr name="layout_width" format="dimension" />
        <attr name="layout_height" format="dimension" />
        <attr name="background" format="string" />
        <attr name="padding" format="dimension" />
        <attr name="paddingLeft" format="dimension" />
        <attr name="paddingTop" format="dimension" />
        <attr name="paddingRight" format="dimension" />
        <attr name="paddingBottom" format="dimension" />
        <attr name="layout_margin" format="dimension" />
        <attr name="layout_marginLeft" format="dimension" />
        <attr name="layout_marginTop" format="dimension" />
        <attr name="layout_marginRight" format="dimension" />
        <attr name="layout_marginBottom" format="dimension" />
        <attr name="minWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="gravity">
        	<enum name="left" value="0x03" />
	        <enum name="right" value="0x05" />
	        <enum name="top" value="0x30" />
	        <enum name="bottom" value="0x50" />
	        <enum name="center_horizontal" value="0x01" />
	        <enum name="center_vertical" value="0x10" />
	        <enum name="center" value="0x11" />
        </attr>
        <attr name="layout_gravity">
	        <enum name="top" value="0x30" />
	        <enum name="bottom" value="0x50" />
	        <enum name="left" value="0x03" />
	        <enum name="right" value="0x05" />
	        <enum name="center_vertical" value="0x10" />
	        <enum name="fill_vertical" value="0x70" />
	        <enum name="center_horizontal" value="0x01" />
	        <enum name="fill_horizontal" value="0x07" />
	        <enum name="center" value="0x11" />
	        <enum name="fill" value="0x77" />
	        <enum name="clip_vertical" value="0x80" />
	        <enum name="clip_horizontal" value="0x08" />
	        <enum name="start" value="0x00800003" />
	        <enum name="end" value="0x00800005" />
  	    </attr>
        <attr name="scrollX" format="dimension" />
        <attr name="scrollY" format="dimension" />
        <attr name="focusable" format="boolean" />
        <attr name="clickable" format="boolean" />
        <!-- <attr name="visibility" format="string" /> -->
        <attr name="visibility">
	        <enum name="visible" value="0" />
	        <enum name="invisible" value="1" />
	        <enum name="gone" value="2" />
    	</attr>
    	<attr name="tag" format="string" />
    </declare-styleable>
    
    <declare-styleable name="ImageView">
    	<attr name="src" format="string" />
        <attr name="scaleType">
        	<enum name="matrix" value="0" />
	        <enum name="fitXY" value="1" />
	        <enum name="fitStart" value="2" />
	        <enum name="fitCenter" value="3" />
	        <enum name="fitEnd" value="4" />
	        <enum name="center" value="5" />
	        <enum name="centerCrop" value="6" />
	        <enum name="centerInside" value="7" />
        </attr>
    </declare-styleable>
    
    <declare-styleable name="TextView">
    	<attr name="text" format="string" />
        <attr name="singleLine" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="maxLines" format="integer" />
        <attr name="hint" format="string" />
        <attr name="textStyle" > 
    		<enum name="normal" value="0" />
    		<enum name="bold" value="1" />
    		<enum name="italic" value="2" />
    	</attr>
    	<attr name="drawableLeft" format="reference" />
    	<attr name="drawableTop" format="reference" />
    	<attr name="drawableRight" format="reference" />
    	<attr name="drawableBottom" format="reference" />
    	<attr name="drawableStart" format="reference" />
    	<attr name="drawableEnd" format="reference" />
    	<attr name="drawablePadding" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="CompoundButton">
    	<attr name="button" format="string" />
        <attr name="checked" format="boolean" />
    </declare-styleable>
    
    <declare-styleable name="SeekBar">
    	<attr name="max" format="integer" />
    	<attr name="progress" format="integer" />
        <attr name="secondaryProgress" format="integer" />
        <attr name="progressDrawable" format="integer" />
        <attr name="thumb" format="integer" />
    </declare-styleable>
    
    <declare-styleable name="GridView">
    	<attr name="numColumns" > 
    		<enum name="autofit" value="-1" />
    	</attr>
        <attr name="columnWidth" format="boolean" />
    </declare-styleable>
    
    <declare-styleable name="FrameLayout">
        <attr name="layout_gravity" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="RelativeLayout">
        <attr name="layout_below" format="string" />
        <attr name="layout_above" format="dimension" />
        <attr name="layout_toLeftOf" format="dimension" />
        <attr name="layout_toRightOf" format="string" />
        <attr name="layout_alignBaseline" format="dimension" />
        <attr name="layout_alignBottom" format="dimension" />
        <attr name="layout_alignTop" format="dimension" />
        <attr name="layout_alignLeft" format="dimension" />
        <attr name="layout_alignRight" format="dimension" />
        <attr name="layout_alignParentBottom" format="dimension" />
        <attr name="layout_alignParentTop" format="dimension" />
        <attr name="layout_alignParentLeft" format="dimension" />
        <attr name="layout_alignParentRight" format="dimension" />
        <attr name="layout_centerHorizontal" format="dimension" />
        <attr name="layout_centerVertical" format="dimension" />
        <attr name="layout_centerInParent" format="dimension" />
        <attr name="layout_gravity" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="LinearLayout">
        <attr name="orientation" format="string" />
        <attr name="layout_weight" format="dimension" />
        <attr name="weightSum" format="int" />
        <attr name="layout_gravity" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="AdapterView">
        <attr name="listSelector" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="ListView">
        <attr name="divider" format="reference" />
        <attr name="dividerHeight" format="int" />
    </declare-styleable>
    
    <declare-styleable name="ProgressBar">
        <attr name="indeterminateDrawable" format="string" />
        <attr name="isBackgroundFaded" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="Animation">
        <attr name="duration" format="int" />
        <attr name="startOffset" format="int" />
        <attr name="fillEnabled" format="boolean" />
        <attr name="fillBefore" format="boolean" />
        <attr name="fillAfter" format="boolean" />
        <attr name="repeatCount" format="int" />
        <attr name="repeatMode" format="int" />
        <attr name="zAdjustment" format="int" />
        <attr name="background" format="dimension" />
        <attr name="detachWallpaper" format="dimension" />
        <attr name="interpolator" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="AlphaAnimation">
        <attr name="fromAlpha" format="float" />
        <attr name="toAlpha" format="float" />
    </declare-styleable>
    
    <declare-styleable name="TranslateAnimation">
        <attr name="fromXDelta" format="float" />
        <attr name="toXDelta" format="float" />
        <attr name="fromYDelta" format="float" />
        <attr name="toYDelta" format="float" />
    </declare-styleable>
    
    <declare-styleable name="RotateAnimation">
        <attr name="fromDegrees" format="float" />
        <attr name="toDegrees" format="float" />
        <attr name="pivotX" format="float" />
        <attr name="pivotY" format="float" />
    </declare-styleable>

    <declare-styleable name="ScaleAnimation">
        <attr name="fromXScale" format="float" />
        <attr name="toXScale" format="float" />
        <attr name="fromYScale" format="float" />
        <attr name="toYScale" format="float" />
        <attr name="pivotX" format="float" />
        <attr name="pivotY" format="float" />
    </declare-styleable>
    
	<declare-styleable name="AccelerateInterpolator">
        <attr name="factor" format="float" />
    </declare-styleable>
    
    <declare-styleable name="AnticipateInterpolator">
        <attr name="tension" format="float" />
    </declare-styleable>
    
    <declare-styleable name="AnticipateOvershootInterpolator">
        <attr name="tension" format="float" />
        <attr name="extraTension" format="float" />
    </declare-styleable>
    
    <declare-styleable name="CycleInterpolator">
        <attr name="cycles" format="float" />
    </declare-styleable>
    
    <declare-styleable name="DecelerateInterpolator">
        <attr name="factor" format="float" />
    </declare-styleable>
    
    <declare-styleable name="OvershootInterpolator">
        <attr name="tension" format="float" />
    </declare-styleable>
    
    <declare-styleable name="AnimationSet">
        <attr name="shareInterpolator" format="reference" />
        <attr name="duration" format="float" />
        <attr name="fillBefore" format="float" />
        <attr name="fillAfter" format="float" />
        <attr name="repeatMode" format="float" />
        <attr name="startOffset" format="float" />
    </declare-styleable>
    
    <declare-styleable name="Menu">
        <attr name="id" format="int" />
        <attr name="icon" format="int" />
        <attr name="title" format="string" />
    </declare-styleable>
    
    <declare-styleable name="ActionBar">
        <attr name="showAsAction">
        	<enum name="always" value="0x03" />
	        <enum name="withText" value="0x05" />
	        <enum name="never" value="0x30" />
	        <enum name="ifRoom" value="0x50" />
        </attr>
        <attr name="orderInCategory" format="int" />
        <attr name="actionLayout" format="int" />
    </declare-styleable>
    
</resources>